{
    "name": "SwarmClientVSCode",
    "displayName": "SwarmClientVSCode",
    "description": "Swarm debugger for VSCode to C# language based on omnisharp debugging log.",
    "author": {
        "name": "Eduardo Andreetta Fontana",
        "email": "fontanadnb@gmail.com"
    },
    "version": "0.0.1",
    "publisher": "eduardoafontana",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Debuggers"
    ],
    "activationEvents": [
        "onDebug"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
